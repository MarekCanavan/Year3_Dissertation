<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diss_cbt_application">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".JournalFeature.JActivities_Fragments.MyJournals.JournalData"></activity>
        <activity android:name=".GoalsFeature.GNewEditGoal" />
        <activity android:name=".JournalFeature.JActivities_Fragments.MyJournals.MyJournalsFragment" />
        <activity android:name=".JournalFeature.JActivities_Fragments.Entries.JournalEntryData"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name=".JournalFeature.JActivities_Fragments.Entries.JournalCompleteEntryActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity
            android:name=".JournalFeature.JActivities_Fragments.Entries.JournalNewStructure"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".JournalFeature.JActivities_Fragments.Entries.JournalChooseActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <receiver android:name=".Notifications.AlertReceiver" />
    </application>

</manifest>